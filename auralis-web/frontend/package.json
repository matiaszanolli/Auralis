{
  "name": "auralis-web-frontend",
  "version": "1.0.0-beta.12.1",
  "description": "Modern web frontend for Auralis audio processing",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@hello-pangea/dnd": "^18.0.1",
    "@mui/icons-material": "^5.14.0",
    "@mui/material": "^5.14.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/wavesurfer.js": "^6.0.0",
    "axios": "^1.5.0",
    "framer-motion": "^12.23.24",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.0",
    "react-query": "^3.39.0",
    "react-router-dom": "^6.15.0",
    "react-virtualized": "^9.22.0",
    "tone": "^14.7.0",
    "typescript": "^4.9.0",
    "wavesurfer.js": "^7.0.0",
    "web-vitals": "^3.0.0"
  },
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "NODE_ENV=test vitest",
    "test:ui": "NODE_ENV=test vitest --ui",
    "test:run": "NODE_ENV=test vitest run",
    "test:memory": "NODE_ENV=test node --max-old-space-size=2048 --expose-gc node_modules/.bin/vitest run",
    "test:coverage": "NODE_ENV=test vitest run --coverage",
    "test:coverage:memory": "NODE_ENV=test node --max-old-space-size=2048 --expose-gc node_modules/.bin/vitest run --coverage",
    "test:watch": "NODE_ENV=test vitest watch",
    "test:player": "NODE_ENV=test vitest run src/tests/integration/player-controls/",
    "test:library": "NODE_ENV=test vitest run src/tests/integration/library-management/",
    "test:streaming": "NODE_ENV=test vitest run src/tests/integration/streaming-audio/",
    "test:websocket": "NODE_ENV=test vitest run src/tests/integration/websocket-realtime/",
    "test:enhancement": "NODE_ENV=test vitest run src/tests/integration/enhancement-processing/",
    "test:errors": "NODE_ENV=test vitest run src/tests/integration/error-handling/",
    "test:performance": "NODE_ENV=test vitest run src/tests/integration/performance/",
    "test:pagination": "NODE_ENV=test vitest run src/tests/integration/performance/pagination.test.tsx",
    "test:virtual-scrolling": "NODE_ENV=test vitest run src/tests/integration/performance/virtual-scrolling.test.tsx",
    "test:cache": "NODE_ENV=test vitest run src/tests/integration/performance/cache-efficiency.test.tsx",
    "test:bundle": "NODE_ENV=test vitest run src/tests/integration/performance/bundle-size.test.tsx",
    "test:memory-mgmt": "NODE_ENV=test vitest run src/tests/integration/performance/memory-management.test.tsx",
    "test:search": "NODE_ENV=test vitest run src/tests/integration/library-management/search.test.tsx",
    "test:filter": "NODE_ENV=test vitest run src/tests/integration/library-management/filter.test.tsx",
    "test:sort": "NODE_ENV=test vitest run src/tests/integration/library-management/sort.test.tsx",
    "test:accessibility": "NODE_ENV=test vitest run src/tests/integration/library-management/accessibility.test.tsx",
    "test:integration": "NODE_ENV=test vitest run src/tests/integration/"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.19.21",
    "@types/react-virtualized": "^9.22.3",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "happy-dom": "^20.0.2",
    "jsdom": "^27.0.0",
    "msw": "^2.6.5",
    "vite": "^7.2.4",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^3.2.4"
  },
  "proxy": "http://localhost:8000"
}
